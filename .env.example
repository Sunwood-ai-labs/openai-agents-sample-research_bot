# OpenAI API設定
# OpenAI APIキーを https://platform.openai.com/api-keys から取得してください
OPENAI_API_KEY="your-api-key-here"

# モデル設定
# 利用可能なモデルについては https://platform.openai.com/docs/models を参照してください
MODEL_NAME="gpt-4o"

# アプリケーション設定
# 必要に応じてカスタマイズしてください
MAX_SEARCHES=10
SEARCH_TIMEOUT=30
REPORT_MIN_WORDS=1000
